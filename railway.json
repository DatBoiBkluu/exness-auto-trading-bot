{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "setup": {
          "nixPkgs": ["python311", "git"]
        },
        "install": {
          "cmds": [
            "pip install -r requirements.txt"
          ]
        },
        "start": {
          "cmd": "python bot.py"
        }
      }
    }
  }
}